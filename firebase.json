{
	"emulators": {
		"dataconnect": {
			"dataDir": "dataconnect/.dataconnect/pgliteData"
		}
	},
	"dataconnect": {
		"source": "dataconnect"
	},
	"firestore": {
		"rules": "firestore.rules",
		"indexes": "firestore.indexes.json"
	},
	"functions": [
		{
			"source": "functions",
			"codebase": "default",
			"predeploy": ["npm --prefix \"$RESOURCE_DIR\" run build"]
		},
		{
			"source": "properpicks-fb",
			"codebase": "properpicks-fb",
			"ignore": [
				"node_modules",
				".git",
				"firebase-debug.log",
				"firebase-debug.*.log",
				"*.local"
			]
		}
	],
	"hosting": {
		"public": "out",
		"site": "bracket-analysis-papagpt",
		"ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
		"rewrites": [
			{
				"source": "/brackets/view/**",
				"destination": "/brackets/view/fallback.html"
			},
			{
				"source": "/profile",
				"destination": "/data-dashboard/index.html"
			},
			{
				"source": "**",
				"destination": "/index.html"
			}
		],
		"redirects": [
			{
				"source": "/profile",
				"destination": "/data-dashboard",
				"type": 301
			}
		]
	},
	"storage": {
		"rules": "storage.rules"
	}
}
