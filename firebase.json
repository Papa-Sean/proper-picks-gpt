{
	"emulators": {
		"dataconnect": {
			"dataDir": "dataconnect/.dataconnect/pgliteData"
		}
	},
	"dataconnect": {
		"source": "dataconnect"
	},
	"firestore": {
		"rules": "firestore.rules",
		"indexes": "firestore.indexes.json"
	},
	"functions": [
		{
			"source": "functions",
			"codebase": "default",
			"predeploy": ["npm --prefix \"$RESOURCE_DIR\" run build"]
		},
		{
			"source": "properpicks-fb",
			"codebase": "properpicks-fb",
			"ignore": [
				"node_modules",
				".git",
				"firebase-debug.log",
				"firebase-debug.*.log",
				"*.local"
			]
		}
	],
	"hosting": {
			"site": "bracket-analysis-papagpt",
			"public": ".next",
			"cleanUrls": true,
			"ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
			"rewrites": [
				{
					"source": "**",
					"destination": "/index.html"
				}
			],
			"headers": [
				{
					"source": "**/*.@(js|css)",
					"headers": [
						{
							"key": "Cache-Control",
							"value": "max-age=31536000"
						}
					]
				},
				{
					"source": "**/*.@(jpg|jpeg|gif|png|svg|webp)",
					"headers": [
						{
							"key": "Cache-Control",
							"value": "max-age=31536000"
						}
					]
				}
			]
		},
	"storage": {
		"rules": "storage.rules"
	}
}
