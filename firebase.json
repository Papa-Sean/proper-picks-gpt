{
	"emulators": {
		"dataconnect": {
			"dataDir": "dataconnect/.dataconnect/pgliteData"
		}
	},
	"dataconnect": {
		"source": "dataconnect"
	},
	"firestore": {
		"rules": "firestore.rules",
		"indexes": "firestore.indexes.json"
	},
	"functions": [
		{
			"source": "functions",
			"codebase": "default",
			"predeploy": ["npm --prefix \"$RESOURCE_DIR\" run build"]
		},
		{
			"source": "properpicks-fb",
			"codebase": "properpicks-fb",
			"ignore": [
				"node_modules",
				".git",
				"firebase-debug.log",
				"firebase-debug.*.log",
				"*.local"
			]
		}
	],
	"hosting": {
		"site": "bracket-analysis-papagpt",
		"source": ".",
		"ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
		"frameworksBackend": {
			"region": "us-central1"
		}
	},
	"storage": {
		"rules": "storage.rules"
	}
}
